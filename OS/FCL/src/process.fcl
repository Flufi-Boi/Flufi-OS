import clone;

struct Process {
    Clone clone;
    bool active = true;

    void constructor(str code, CloneData data) {
        self.clone = new Clone("fbl", code, data);
    }

    void kill() {
        self.active = false;
        self!.clone.kill();
    }
    void continue() {
        self!.clone.continue();
    }

    auto getScope<t>() {
        return clone:getCloneData<t>(self!.clone!.id);
    }
}
