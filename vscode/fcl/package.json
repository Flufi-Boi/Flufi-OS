{
  "name": "fcl",
  "displayName": "FCL",
  "description": "adds .fcl (flufi compiled language) support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "fcl",
        "aliases": [
          "fcl",
          "fcl"
        ],
        "extensions": [
          ".fcl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fcl",
        "scopeName": "source.fcl",
        "path": "./syntaxes/fcl.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "vscode": "^1.1.37"
  }
}
